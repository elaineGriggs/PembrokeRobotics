#include <kipr/botball.h>

int main()
{
   motor(0, 100);
    motor(1, 100);
    msleep(2000);  

	motor(0, 1);
    motor(1, 100);
    msleep(900);
    
    motor(0, 100);
    motor(1, 100);
    msleep(3000);
   
    motor(0, 100);
    motor(1, 1);
    msleep(2000);
    
     motor(0, 100);
    motor(1, 100);
    msleep(10000);
    
}



